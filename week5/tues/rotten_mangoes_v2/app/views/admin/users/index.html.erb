<h1>Admin Section</h1>
<%= paginate @users %>

<% @users.each do |user| %>
  
 <table class="table">
  <thead>
    <tr>

      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    <tr>

<td><%= user.firstname %></td>
<td><%= user.lastname %></td>
<td><%= user.email %></td>
    </tr>

  </tbody>
</table>

  <% end %>
