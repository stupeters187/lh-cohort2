<h1><%= @song.title %></h1>

<p>
  Title: <%= @song.title %>
</p>
<p>
  Artist: <%= @song.artist %>
</p>
<p>
  Url:<a href="<%= @song.url %>"><%= @song.url %></a>

</p>
<p>
  Added by: <% @song_user = User.find_by(@song.user_id) %> <%= @song_user.username %>
</p>
<p>
  Created: <%= @song.created_at.to_s(:short) %>
</p>
<p>
  Last Updated: <%= @song.updated_at.to_s(:short) %>
</p>

<hr/>
<a href="/songs">Back to All Songs</a>